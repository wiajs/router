{
  "name": "@wiajs/router",
  "version": "1.0.19",
  "description": "Official router for wia.js",
  "main": "./dist/router.cjs",
	"module": "./dist/router.mjs",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/router.cjs",
      "import": "./dist/router.mjs",
      "default": "./dist/router.mjs",
      "types": "./types/index.d.ts"
    }
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development gulp build -f gulpfile.mjs",
    "build": "cross-env NODE_ENV=production gulp build -f gulpfile.mjs",
    "lib": "swc --config-file ./.swcrc ./src -w -d lib",
    "dev:umd": "cross-env NODE_ENV=development gulp umd",
    "dev:esm": "cross-env NODE_ENV=development gulp esm",
    "release": "bash release.sh"    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wiajs/router.git"
  },
  "keywords": [
    "wia",
    "router"
  ],
  "author": {
    "name": "Sibyl Yu",
    "email": "sibyl@wia.pub"
  },
  "license": "ELv2",
  "bugs": {
    "url": "https://github.com/wiajs/dom/issues"
  },
  "homepage": "https://www.wia.pub/router",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@wiajs/dom": "^1.6.1",
    "@wiajs/core": "^1.1.28",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.1",
    "gulp": "^4.0.2",
    "rollup": "^3.25.1",
    "terser": "^5.18.0"
  },
  "dependencies": {}
}
